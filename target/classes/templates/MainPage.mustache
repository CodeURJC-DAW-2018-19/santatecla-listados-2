<body>

{{> header}}

<!--   Search Box   -->

<div class="search_box_padding">
    <div class="search_box">
        <form class="search_box--form" action="/MainPage/search" method="post">
            <div class="row">
                <div class="col-md-11">
                    <div class="form-group bmd-form-group">
                        <div class="input-group-prepend">
                            <input type="text" name=searchText id=searchText class="form-control" placeholder="Search" style="padding: 0px 10px">
                        </div>
                    </div>
                </div>
            </div>
            <input class="search_box_icon--submit" name=result type="submit" value=" &#9906;"
                   style="-webkit-transform: rotate(45deg);"/>
            <input type="hidden" name="_csrf" value="{{token}}"/>
        </form>
    </div>
</div>


<div class="bodyP card">
    <!--   Temary (Table)   -->
    <div class="container">
        <div id="accordion">
            {{#topics}}
                <div class="card">
                    <div class="card-header">
                        <a class="card-link" data-toggle="collapse" href="#collapse{{-index}}">
                            <font size="5"><b>TEMA {{-index}}: {{name}}
                                {{#student}}
                                    <div style="float: right;">({{hits}},{{pendings}},{{errors}}):{{total}}</div>
                                {{/student}}
                            </b></font>
                        </a>
                        {{#teacher}}
                            <a href="/MainPage" style="position:relative;float:right ;right: 15%;">
                                <i class="material-icons">delete</i>
                            </a>
                        {{/teacher}}
                    </div>

                    <div id="collapse{{-index}}" class="collapse" data-parent="#accordion">
                        {{#concepts}}
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-md-5">
                                        <a class="nav-link" {{#student}}href="/MainPage/Student/{{name}}"{{/student}}{{#teacher}}href="/MainPage/Teacher/{{name}}"{{/teacher}}>{{name}}</a>
                                    </div>
                                    {{#teacher}}
                                        <a href="/MainPage" style="position: relative;left: 42.8%;">
                                            <i class="material-icons">delete</i>
                                        </a>
                                    {{/teacher}}
                                    {{#student}}
                                        <div style="position: relative;right: -35%;">({{hits}},{{pendings}},{{errors}}):{{total}}</div>
                                    {{/student}}
                                </div>
                            </div>
                        {{/concepts}}
                        {{#teacher}}
                            <button class="btn btn-primary btn-fab btn-fab-mini btn-round" style="position: relative;left: 15%">
                                <i class="material-icons">add</i>
                            </button>
                        {{/teacher}}
                    </div>

                </div>
            {{/topics}}
            {{#teacher}}
                <button class="btn btn-primary btn-fab btn-fab-mini btn-round" style="position: relative;left: 2%">
                    <i class="material-icons">add</i>
                </button>
            {{/teacher}}
        </div>
    </div>

</div>

<!--   Diagram Botton   -->
{{^guest}}
{{> diagram}}
{{/guest}}
{{> footer}}