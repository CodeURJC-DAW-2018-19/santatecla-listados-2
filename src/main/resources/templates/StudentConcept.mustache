<body  style="overflow-x:hidden">

{{> header}}


<div id="contained" style="margin: 2%">
    <div id="tobeseen">
        <h3>PREGUNTAS CORREGIDAS</h3>
        <table border="0" cellspacing="20" id="corrected">
             {{#questions}}
                 {{#corrected}}
            <tr>
                <td>{{question}}: <a href="#" data-toggle="modal" data-target="#modal{{type}}" > {{#answer}}{{answerTest}} {{/answer}} {{-index}}</a></td>
                {{#answer}}
                    {{#mark}}
                        <td><i class="material-icons" style="color:green">check</i></td>
                    {{/mark}}
                    {{^mark}}
                        <td><i class="material-icons" style="color:red">clear</i></td>
                    {{/mark}}
                {{/answer}}
            </tr>
                 {{/corrected}}
             {{/questions}}
        </table>
    </div>

    <div id="answered">
        <h3>PREGUNTAS POR CORREGIR</h3>
        {{#questions}}
            {{^corrected}}
        <ul class="list-group" style="list-style: none;">
            <li>{{question}}: <a href="#" data-toggle="modal" data-target="#modal{{type}}">{{#answer}}{{answerTest}} {{/answer}} {{-index}}</a></li>
        </ul>
            {{/corrected}}
        {{/questions}}
    </div>
</div>

<div class="modal fade" id="modalPType0" tabindex="-1" role="dialog" aria-labelledby="exampleModalPAType0"
     aria-hidden="true" align="center">
    <div class="modal-dialog " role="document">
        <div class="modal-content">
            <div class="card card-plain">
                <div class="modal-header card-header card-header-primary">
                    <h4 class="card-title">Pregunta X</h4>
                </div>
            </div>
            <div class="modal-body" align="left">
                <form>
                    <div class="row">
                        <div class="col-md-10">
                            <div class="form-group bmd-form-group">
                                <textarea name="answerTest" rows="3" cols="49" placeholder="Respuesta"></textarea>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="button-container" align="right">
                    <button class="btn btn-primary btn-fab btn-fab-mini btn-round">
                        <i class="material-icons">add</i>
                    </button>
                </div>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-dismiss="modal">Aceptar</button>
            </div>
        </div>
    </div>
</div>
